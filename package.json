{
  "name": "root",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "pnpm run format:prettier && npm run format:stylelint",
    "format:prettier": "prettier --check --write --ignore-unknown --ignore-path .prettierignore \"{website,packages}/**/*.{[jt]s?(x),m[jt]s,json,md}\"",
    "format:stylelint": "stylelint --fix --ignore-path .prettierignore --quiet \"{website,packages}/**/*.{css,scss}\""
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.14.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "stylelint": "^16.10.0",
    "stylelint-config-clean-order": "6.1.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "stylelint-prettier": "5.0.2",
    "stylelint-scss": "^6.9.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "pnpm": {
    "overrides": {
      "@react-router/node": "7.0.0-pre.5",
      "@react-router/dev": "7.0.0-pre.5",
      "@types/react": "^18.3.12",
      "@types/react-dom": "^18.3.1",
      "esbuild": "^0.24.0",
      "react": "19.0.0-rc-ed15d500-20241110",
      "react-dom": "19.0.0-rc-ed15d500-20241110",
      "react-router": "7.0.0-pre.5"
    }
  },
  "engines": {
    "node": ">=20"
  }
}