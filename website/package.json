{
  "name": "website",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "tw-patch extract && tw-patch install",
