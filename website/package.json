    "postcss": "^8.4.49",
    "postcss-load-config": "^6.0.1",
    "sass-embedded": "^1.81.0",
    "tailwindcss": "^3.4.15",
    "tailwindcss-patch": "^5.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "unplugin-tailwindcss-mangle": "^4.0.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "engines": {
    "node": ">=20"
  }
